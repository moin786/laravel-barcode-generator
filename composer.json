{
    "name": "peal/laravel-barcode-generator",
    "description": "Barcode generate in Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Moinuddin Chowdhury",
            "email": "moinuddin7@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Peal\\BarCodeGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Peal\\BarCodeGenerator\\BarCodeServiceProvider"
            ],
            "aliases": {
                "BarCode": "Peal\\BarCodeGenerator\\Facades\\BarCode"
            }
        }
    },
    "minimum-stability": "stable",
    "require": {
        "laravel/framework": "5.2.*|5.6.*|5.7.*|5.8.*|6.*|7.*|8.*|9.*|10.*|11.*"
    }
}
